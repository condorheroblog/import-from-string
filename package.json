{
	"name": "import-from-string",
	"type": "module",
	"version": "0.0.4",
	"packageManager": "pnpm@9.12.1+sha512.e5a7e52a4183a02d5931057f7a0dbff9d5e9ce3161e33fa68ae392125b79282a8a8a470a51dfc8a0ed86221442eb2fb57019b0990ed24fab519bf0e1bc5ccfc4",
	"description": "Load module from string using require or import.",
	"imports": {
		"#*": "./*"
	},
	"author": "CondorHero",
	"license": "MIT",
	"homepage": "https://github.com/condorheroblog/import-from-string#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/condorheroblog/import-from-string.git"
	},
	"bugs": {
		"url": "https://github.com/condorheroblog/import-from-string/issues"
	},
	"keywords": [
		"module-from-string",
		"load-from-string",
		"require-from-string",
		"import-from-string"
	],
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.cts",
	"typesVersions": {
		"*": {
			"*": [
				"./dist/*",
				"./dist/index.d.ts"
			]
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"prepublishOnly": "nr build",
		"dev": "esno index.ts",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"test": "vitest",
		"build": "rimraf dist && rollup -c",
		"build:watch": "rimraf dist && rollup -c -w",
		"typecheck": "tsc --noEmit",
		"release": "bumpp -r && pnpm -r publish --access public"
	},
	"dependencies": {
		"esbuild": "^0.24.0",
		"import-meta-resolve": "^4.1.0"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^3.8.0",
		"@rollup/plugin-json": "^6.1.0",
		"@types/node": "^22.9.0",
		"bumpp": "^9.8.1",
		"eslint": "^9.14.0",
		"esno": "^4.8.0",
		"lint-staged": "^15.2.10",
		"rimraf": "^6.0.1",
		"rollup": "^4.24.4",
		"rollup-plugin-dts": "^6.1.1",
		"rollup-plugin-esbuild": "^6.1.1",
		"rollup-plugin-node-externals": "^7.1.3",
		"simple-git-hooks": "^2.11.1",
		"typescript": "^5.6.3",
		"vitest": "^2.1.4"
	},
	"simple-git-hooks": {
		"pre-commit": "npx lint-staged"
	},
	"lint-staged": {
		"*.{ts,js,mjs,cjs,json,md}": [
			"eslint --fix"
		]
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org"
	}
}
